package com.multi.mvc03;

import org.springframework.stereotype.Component;

@Component
public class BSite {
	
	public void search() {
		System.out.println("물건검색하다.");
	}
	public void basket() {
		System.out.println("장바구니에 넣다.");
	}
	public void productOrder() {
		System.out.println("물건을 주문하다.");
	}
}
